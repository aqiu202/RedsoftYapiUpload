<templateSet group="YApi">
    <template name="-yc" value="**&#x000D;
 * @author $user$&#x000D;
 * @date $date$ $time$&#x000D;
 * @description //TODO&#x000D;
 * @menu all&#x000D;
**/" description="yapi class template" toReformat="true" toShortenFQNames="true">
        <variable name="user" expression="user()" defaultValue="" alwaysStopAt="true" />
        <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
        <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="JAVA" value="true" />
        </context>
    </template>
    <template name="-ym" value="**&#x000D;
 * @author $user$&#x000D;
 * @date $date$ $time$&#x000D;
 * @description //TODO$param$&#x000D;
 * @return {@link $return$}&#x000D;
**/" description="yapi method template" toReformat="true" toShortenFQNames="true">
        <variable name="user" expression="user()" defaultValue="" alwaysStopAt="true" />
        <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
        <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
        <variable name="param" expression="" defaultValue="groovyScript(&quot;
            if(\&quot;${_1}\&quot;.length() == 2) {return &apos;&apos;;}
            else {def result=&apos;&apos;;
            def params=\&quot;${_1}\&quot;.replaceAll(&apos;[\\\\[|\\\\]|\\\\s]&apos;, &apos;&apos;).split(&apos;,&apos;).toList();
            for(i = 0; i &lt; params.size(); i++) {result+=&apos;\\n&apos; + &apos; * @param &apos; + params[i] + &apos;: &apos;};
            return result;}&quot;, methodParameters());
" alwaysStopAt="true" />
        <variable name="return" expression="methodReturnType()" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="JAVA" value="true" />
        </context>
    </template>
</templateSet>
